<?xml version="1.0"?>
<ivysettings>
  <settings defaultResolver="default"/>
  <property name="ivy.checksums" value=""/>

  <resolvers>
    <chain name='default'>
      <ibiblio name='maven2'
               root='http://mirrors.ibiblio.org/pub/mirrors/maven2/'
	       pattern='[organisation]/[module]/[revision]/[module]-[revision].[ext]'
               m2compatible='true' />
	  <!-- jogl isn't in the ibiblio repository -->
	  <ibiblio name="java.net"
	  	root="http://download.java.net/maven/2/"
        pattern="[organization]/[module]/[revision]/[module]-[revision].[ext]" 
		m2compatible='true' />
    </chain>
  </resolvers>

</ivysettings>

